alias adb='sudo -i `command -v adb`'
alias dchroot='TERM=xterm dchroot'
alias ec=emacsclient
alias man='LANG=C man'
alias pstree='pstree -A'
alias screen='TERM=screen screen'
alias tm="timidity -OjS -c $HOME/.timidity.cfg"
alias tm16="timidity -OjS -c $HOME/.timidity.cfg --output-16bit"
alias tm24="timidity -c $HOME/.timidity.cfg --output-24bit"
alias vi='vim'
alias which='command -v'
alias xc='xclip -selection clipboard'
alias xp='xclip -selection primary'
alias xs='xclip -selection secondary'
